{
  "name": "toisu-static",
  "version": "1.0.0",
  "description": "A simple wrapper around serve-static for Toisu!",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage",
    "lint": "eslint .",
    "test-node": "tape 'test/**/*.js'",
    "cover": "istanbul cover tape 'test/**/*.js'",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "test": "npm run clean && npm run lint && npm run cover && npm run check-coverage && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qubyte/toisu-static.git"
  },
  "keywords": [
    "toisu",
    "static"
  ],
  "author": "Mark S. Everitt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qubyte/toisu-static/issues"
  },
  "homepage": "https://github.com/qubyte/toisu-static#readme",
  "devDependencies": {
    "eslint": "3.6.1",
    "eslint-config-qubyte": "1.0.1",
    "istanbul": "0.4.5",
    "rimraf": "2.5.4",
    "sandboxed-module": "2.0.3",
    "sinon": "1.17.6",
    "tape": "4.6.0"
  },
  "dependencies": {
    "serve-static": "1.11.1"
  }
}
